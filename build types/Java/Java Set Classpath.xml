<?xml version="1.0" encoding="UTF-8"?>
<BuildType>
  <Version>6.3</Version>
  <OperatingSystem>Java</OperatingSystem>
  <Name>Java Set Classpath</Name>
  <Description>Final Results Extension: .classpath
Builds a .classpath file. It uses the Set Classpath to resolve all classpath files. The result can then be included in a different TGT.</Description>
  <FinalTargetExt>.classpath</FinalTargetExt>
  <BuildTasks>
    <Name>Set Classpath</Name>
    <OptionGroups>
      <GroupName>Build Task Options</GroupName>
      <Type>1</Type>
    </OptionGroups>
  </BuildTasks>
  <Rules>
    <ParentBuildTask>Set Classpath</ParentBuildTask>
    <TargetExt>.classpath</TargetExt>
    <SourceExts>.zip .jar .properties</SourceExts>
    <NameFrom>Final Target</NameFrom>
    <CompilerName>Set Classpath.sc</CompilerName>
    <Makefile>Java</Makefile>
    <ToDir>Final Target</ToDir>
    <OptionGroup>
      <GroupName>Rule Options</GroupName>
      <Type>2</Type>
    </OptionGroup>
  </Rules>
</BuildType>
